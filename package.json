    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.10",
    "ts-jest": "29.1.2",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
