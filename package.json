    "babel-plugin-styled-components": "2.1.4",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.10.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.0",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
